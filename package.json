    "dev": "npm run clear && cross-env NODE_ENV=development webpack --mode development --watch",
    "build": "npm run clear && npm run lint && cross-env NODE_ENV=production webpack --mode production",
    "lint": "tsc --noEmit && prettier --check . && eslint --ext 'ts' --ext 'tsx' src/ && stylelint '**/*.css'",
    "prepare": "husky install && git config --local commit.template .husky/.gitcommittemplate",
    "docker-build": "docker build -t galaga .",
    "docker-run": "docker run --init -p 3000:3000 galaga",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "test": "jest",
    "dev:server": "nodemon dist/server/index.js --watch dist/server",
    "clear": "rimraf dist"
  },
