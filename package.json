{
  "name": "galaga",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "dev": "npm run dc:up:dev && npm run local:dev",
    "build": "npm run lint && cross-env NODE_ENV=production webpack --mode production",
    "lint": "tsc --noEmit && prettier --check . && eslint --ext 'ts' --ext 'tsx' src/ && stylelint '**/*.css'",
    "prepare": "husky install && git config --local commit.template .husky/.gitcommittemplate",
    "docker-build": "docker build -t galaga .",
    "docker-run": "docker run --init -p 3000:3000 galaga",
    "dc:up:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "dc:down:dev": "docker-compose -f docker-compose.dev.yml down",
    "dc:logs:dev": "docker-compose -f docker-compose.dev.yml logs",
    "dc:up": "docker-compose -f docker-compose.yml up -d --build",
    "dc:down": "docker-compose -f docker-compose.yml down",
    "dc:logs": "docker-compose -f docker-compose.yml logs",
    "local:dev": "cross-env NODE_ENV=development webpack watch --mode development",
    "start": "cross-env NODE_ENV=production node dist/server/server.*.js",
    "test": "jest"
  },
  "repository": {
